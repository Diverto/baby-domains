{
  "name": "baby-domains",
  "version": "1.0.0",
  "description": "Scrapes baby domains from DomainDS",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "env-cmd -f .env nodemon src/index.js",
    "lint-staged": "lint-staged",
    "test:dev": "env-cmd -f .env.local jest ",
    "test:codecov": "jest --coverage",
    "test:prod": "jest --bail",
    "snyk:test": "snyk test",
    "snyk:monitor": "snyk monitor"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Diverto/baby-domains.git"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Diverto/baby-domains/issues"
  },
  "homepage": "https://github.com/Diverto/baby-domains#readme",
  "devDependencies": {
    "codecov": "^3.5.0",
    "cross-env": "^5.2.0",
    "env-cmd": "^9.0.3",
    "eslint": "^6.2.2",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "mongodb": "^3.3.1",
    "nodemon": "^1.19.1",
    "snyk": "^1.221.0"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^2.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.6.11",
    "validator": "^11.1.0",
    "winston": "^3.2.1"
  }
}
