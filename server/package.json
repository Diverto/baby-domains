{
  "name": "baby-domains",
  "version": "1.0.0",
  "description": "Scrapes baby domains from DomainDS",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:ci": "nodemon src/index.js",
    "dev": "env-cmd -f .env nodemon src/index.js",
    "lint-staged": "lint-staged",
    "test:dev": "env-cmd -f .env.local jest ",
    "test:codecov": "jest --coverage",
    "test:prod": "jest --bail",
    "snyk:test": "snyk test",
    "snyk:monitor": "snyk monitor",
    "test": "snyk test",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Diverto/baby-domains.git"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Diverto/baby-domains/issues"
  },
  "homepage": "https://github.com/Diverto/baby-domains#readme",
  "devDependencies": {
    "codecov": "^3.6.1",
    "cross-env": "^6.0.3",
    "env-cmd": "^10.0.1",
    "eslint": "^6.7.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "mongodb": "^3.3.5",
    "nodemon": "^2.0.1"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^3.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.7.13",
    "snyk": "^1.258.2",
    "validator": "^12.1.0",
    "winston": "^3.2.1"
  },
  "snyk": true
}
