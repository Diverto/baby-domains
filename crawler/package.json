{
  "name": "crawler",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node src/index.js",
    "start:ci": "nodemon src/index.js",
    "dev": "env-cmd -f .env.local nodemon src/index.js",
    "lint-staged": "lint-staged",
    "test:dev": "env-cmd -f .env jest ",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:codecov": "jest --coverage",
    "test:prod": "jest --bail",
    "snyk:test": "snyk test",
    "snyk:monitor": "snyk monitor",
    "test": "snyk test",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add",
      "env-cmd -f .env jest --bail --findRelatedTests"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [],
  "author": "Tomislav Andros",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@google-cloud/logging-winston": "^3.0.0",
    "acorn": "^7.1.0",
    "ajv": "^6.10.2",
    "amqplib": "^0.5.5",
    "cheerio": "^1.0.0-rc.3",
    "cron": "^1.7.2",
    "env-cmd": "^10.0.1",
    "is-html": "^2.0.0",
    "mongodb": "^3.3.5",
    "mongoose": "^5.7.13",
    "p-event": "^4.1.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "rewire": "^4.0.1",
    "snyk": "^1.258.2",
    "unzipper": "^0.10.5",
    "validator": "^12.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "codecov": "^3.6.1",
    "coveralls": "^3.0.9",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.1",
    "stream-mock": "^2.0.5"
  },
